{
    "compilerOptions": {
      "declaration": true,
      "declarationDir": "dist",
      "emitDeclarationOnly": false,
      "outDir": "dist",
      "module": "ESNext",
      "target": "ES2019",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "strict": true
    },
    "include": ["src/**/*"]
  }
  